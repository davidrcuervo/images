<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Images">
		<class>com.laetienda.images.entities.Log</class>
		<class>com.laetienda.images.entities.Text</class>
		<class>com.laetienda.images.entities.Setting</class>
		<class>com.laetienda.images.entities.User</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://devdb.la-etienda.lan:5432/images"/>
			<property name="javax.persistence.jdbc.user" value="images"/>
			<property name="javax.persistence.jdbc.password" value="www.myself.com"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
